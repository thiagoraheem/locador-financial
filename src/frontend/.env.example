# Frontend Environment Variables
# Copy this file to .env and update the values for your environment

# API Configuration
# For development: http://localhost:8001
# For production: https://your-domain.com or http://your-server-ip:port
REACT_APP_API_URL=http://localhost:8001

# Frontend Port (optional)
PORT=5600

# Build Configuration
# Set to false in production to reduce bundle size
GENERATE_SOURCEMAP=false

# Production Environment Variables
# Uncomment and configure for production deployment
# REACT_APP_API_URL=https://api.your-domain.com
# REACT_APP_API_URL=http://your-server-ip:8000
# NODE_ENV=production